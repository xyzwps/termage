{
  "name": "termage",
  "version": "0.0.1",
  "description": "Display an image in console/terminal.",
  "main": "index.mjs",
  "types": "index.d.ts",
  "bin": {
    "termage": "./cli.mjs"
  },
  "files": ["*.mjs", "*.d.ts"],
  "type": "module",
  "keywords": [
    "image",
    "terminal",
    "console",
    "display",
    "png",
    "jpeg",
    "gif",
    "bmp",
    "jpg",
    "ascii"
  ],
  "scripts": {
    "format": "biome format --write *.mjs *.json"
  },
  "author": {
    "name": "xyzwps",
    "email": "xyzwps@outlook.com"
  },
  "license": "MIT",
  "repository": "https://github.com/xyzwps/termage",
  "dependencies": {
    "ansis": "^4.0.0",
    "jimp": "^1.6.0"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4"
  }
}
